{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug RP2350",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "openocd",
      "cwd": "${workspaceFolder}",
      "executable": "${workspaceFolder}/build-arm/floatmark_O3.elf",
      "device": "RP2350",
      "configFiles": [
        "interface/cmsis-dap.cfg",
        "target/rp2350.cfg"
      ],
      "openOCDLaunchCommands": [
        "adapter speed 5000"
      ],
      "runToEntryPoint": "main",
      "preLaunchCommands": [
        "load"
      ],
      "searchDir": [
        "/usr/share/openocd/scripts"
      ],
      "gdbPath": "/usr/bin/gdb-multiarch",
      "serverLaunchTimeout": 5000
    }
  ]
}
